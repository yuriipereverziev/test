include ../../mixins/product-page

-
    var PRODUCT_NAME = 'Guavital+'
    var PRODUCT_CARD = LANG.product_card;
    var CURRENT_PRODUCT = LANG.guavital;
    var RECOMMENDATIONS = LANG.recommendations.filter(prod => prod.name === 'Beauty Age Skin' || prod.name === 'Beauty Age Skin Peeling' || prod.name === 'Steplex')
    var REVIEWS = LANG.reviews.reviews_guavital;
    REVIEWS.length = 3;

+productPage(PRODUCT_NAME, PRODUCT_CARD, CURRENT_PRODUCT, RECOMMENDATIONS, REVIEWS)